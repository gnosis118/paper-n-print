{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "proinvoice.app" }],
      "destination": "https://www.proinvoice.app/$1",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "index, follow"
        }
      ]
    }
  ]
}